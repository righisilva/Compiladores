define var 5

inicia principal()
    inteiro x = 12;
    quebrado y = 1,25;
    
    inicia se (x < var) entao
        imprime "x eh menor que var";
        senao
        imprime "x eh maior que var";
    termina se
    
    inicia para (inteiro z = 0; z <= var; z++)
        imprime "z";
    termina para
    
    inicia enquanto (x != var) faz
        x--;
    termina enquanto
     
termina principal    
